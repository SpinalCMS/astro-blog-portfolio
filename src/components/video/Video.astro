---
import { buildPublicPath } from "@/functions/Media"

// props
export interface Props {
    file: string;
    classes?: string;
}
const { file, classes } = Astro.props;
---

<video width="100%" height="auto" controls>
    <source src={buildPublicPath(file)} type="video/mp4">
    
    Your browser does not support the HTML video tag.
</video>